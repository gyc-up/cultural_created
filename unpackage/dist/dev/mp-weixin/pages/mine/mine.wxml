<view class="page-mask"><view class="center "><view class="setting"><block wx:for="{{itemList}}" wx:for-item="item" wx:for-index="key" wx:key="key"><block wx:if="{{setting}}"><view data-event-opts="{{[['tap',[['setting_modeify',[key]]]]]}}" bindtap="__e"><view class="setting_box"><view class="box_word">{{item.name+''}}</view></view></view></block></block></view><view class="center_top"><view class="mask"></view></view><view class="center_box_bg"><view class="profily"><view class="base"><view class="profily_header"></view><block wx:if="{{nickname==''}}"><text data-event-opts="{{[['tap',[['login']]]]}}" bindtap="__e">点击登陆</text></block><block wx:if="{{nickname!=''}}"><text>{{nickname+''}}</text></block><image src="../../static/fumou-center-template/setting.png" mode data-event-opts="{{[['tap',[['setting_click',['$event']]]]]}}" bindtap="__e"></image></view><view class="order_status"><block wx:for="{{status}}" wx:for-item="item" wx:for-index="__i0__"><view data-event-opts="{{[['tap',[['statusclick',['$0'],[[['status','',__i0__]]]]]]]}}" class="status" bindtap="__e"><image class="icon" src="{{item.url}}" mode="aspectFill"></image><text>{{item.name}}</text></view></block></view></view><view class="baiban"></view><view class="center_menu"><block wx:for="{{menus}}" wx:for-item="item" wx:for-index="__i1__"><view class="menu_item"><image src="{{item.icon}}" mode="aspectFill"></image><text>{{item.name}}</text></view></block></view><view data-event-opts="{{[['tap',[['setting_click']]]]}}" hidden="{{!(showPopup)}}" class="overlay" bindtap="__e"></view></view></view></view>